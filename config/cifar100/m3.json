{
    "dataset": "cifar100",
    "num_classes": 100,
    "pretrained": null,
    "batch_size": 128,
    "num_epochs": 600,
    "feature_dim": 32,
    "lr": 0.001,
    "wd": 0.0,
    "training_type": "stochastic+adversarial",
    "w_ct": 0.5,
    "w_at": 0.5,
    "attack": "pgd",
    "epsilon": 0.03,
    "var_type": "anisotropic",
    "reg_type": "wca+max_entropy",
    "device": "cuda:3",
    "output_path": {
        "stats": "output/stats/a2snn_cifar100_m3",
        "models": "output/models/a2snn_cifar100_m3"
    },
    "monte_carlo_runs": 50
}
